<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.debasish.guitardhun">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.LoginScreen" />
        <activity
            android:name=".activities.SignUpScreen"
            android:label="Registration" />
        <activity
            android:name=".activities.HomeScreen"
            android:label="@string/title_activity_home_screen" />
        <activity android:name=".activities.DetailsScreen"></activity>
    </application>

</manifest>